{% extends 'Admin/base.html.twig' %}

{% block content %}

    <h2>Admin Tag</h2>
<div>
    <table>
        {% for tag in tags  %}
            <tr>
                <td>{{ tag.title }}</td>
                <td>{{ tag.color }}</td>
                <td>{{ tag.id }}</td>
            </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}